/* width */
::-webkit-scrollbar {
    width: 4px;
    height: 4px;
}

/* Track */
::-webkit-scrollbar-track {
    background: #f1f1f1; 
}
 
/* Handle */
::-webkit-scrollbar-thumb {
    background: #888; 
}

/* Handle on hover */
::-webkit-scrollbar-thumb:hover {
    background: #555; 
}

td{
	text-align:left;
	font-size:12px;
	border-bottom:1px solid #c7d1d6;
	color:#697880;
}

:link, :visited{
	text-decoration:none;
	color:#405e79;
}