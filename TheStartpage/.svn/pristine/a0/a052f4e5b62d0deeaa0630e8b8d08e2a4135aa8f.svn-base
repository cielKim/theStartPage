package kr.co.bit.papersearch;

public class paperVO {
	
	private String title;
	private String writer;
	private String link;
	
	
	public paperVO(String title, String writer, String link) {
		super();
		this.title = title;
		this.writer = writer;
		this.link = link;
	}
	
	public paperVO(String title, String link) {
		super();
		this.title = title;
		this.link = link;
	}
	
	public paperVO(String writer) {
		super();
		this.writer = writer;
	}
	
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public String getWriter() {
		return writer;
	}
	public void setWriter(String writer) {
		this.writer = writer;
	}
	public String getLink() {
		return link;
	}
	public void setLink(String link) {
		this.link = link;
	}
	@Override
	public String toString() {
		return "paperVO [title=" + title + ", writer=" + writer + ", link=" + link + "]";
	}
	
	
	
	

}
