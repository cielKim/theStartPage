@font-face{
	font-family: "Kozuka Gothic";
	src:url("../font/Kozuka Gothic Pro EL.otf") format("truetype");
	font-style: normal;
	font-weight: normal;
}

html,
body,
.wrapper,
.page {
    width: 100%;
    height: 100%;
}

body {
    margin: 0;
    position: relative;
}

body{
  overflow: hidden;
  -webkit-transform-style: preserve-3d;
}

.wrapper {
    position: relative;
}

.page {
    display: none;
    position: absolute;
    top: 0;
    left: 0;
    color: #333;
    background: #FFFFFF;
    overflow: hidden;
}

.page.active {
    display: block;
}

#page1{
	background-size:100% 100%;
	background-repeat:no-repeat;
}

#title {
	position:absolute;
	width:30%;
	font-size:200%;
	margin-left:51.5%;
	margin-top:14%;
	text-align:right;
	font-family: "Kozuka Gothic";
}

#fordevelopers{
	font-family:'Source Sans Pro';
	color:white;
	font-size:72px;
	text-shadow: 1px 1px 12px #727b7b;
}

#yourstartpage{
	color:#e4eceb;
	font-size:28px;
	text-shadow: 1px 1px 8px #727b7b;
}

#yourstartpageWrapper{
	margin-top:-10px;
	padding-right:10px;
}

#logo{
	padding-left:200px;
	width: 60%;
	float: right;
	margin-top: 10%;
	padding-left: 30%;
}

#btns{
	width:16%;
	position:absolute;
	top:80%;
	left:44%;
	}
	
#btn1, #btn2{
	width:30%;
	box-shadow:0 10px 30px 0px rgba(32, 32, 32, 0.3);
	border: none;
	border-radius: 50%;
	margin: 10px;
}
.modal {
    display: none; 
    position: fixed; 
    z-index: 1; 
    padding-top: 100px; 
    left: 0;
    top: 0;
    width: 100%; 
    height: 100%;
    overflow: auto; 
    background-color: rgba(0,0,0,0.4);
}

.modal-content{
	line-height:40px;
	border-radius:4px;
	background-color:white;
	padding:20px;
	width:460px;
	height:249px;
	position:fixed;
	top: 35%;
    left: 36.7%;
	text-align: center;
	vertical-align: middle;
	color: #545454;
	font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;
	-webkit-animation-name: animatetop;
    -webkit-animation-duration: 1s;
    animation-name: animatetop;
    animation-duration: 1s;
    background-repeat:no-repeat;
    background-size: 100%;
}

input:focus {
  outline: none;
}
 
@-webkit-keyframes animatetop {
    from {top:-150px; opacity:0} 
    to {top:100; opacity:1}
}

@keyframes animatetop {
    from {top:-150px; opacity:0}
    to {top:100; opacity:1}
}

.close, .close1 {
    color: #545454;
    float: right;
    font-size: 28px;
    font-weight: bold;
}

.close:hover,
.close1:hover,
.close:focus,
.close1:focus {
	text-shadow:1px 1px 5px black;
    text-decoration: none;
    cursor: pointer;
}
.box1{
	border: none;
	background-color:transparent;
	color: #545454;
}
.box2{
	border:none;
	border-radius:4px;
	color: white;
	background-color:#3085d6;
	padding:10px 32px 10px 32px;
	margin: 30px 5px 0px 5px;
}

#page2{
	background: #E5eaee;
	text-align: center;
}

.grid-stack-item-content img{
	width:95%;
	height:95%;
	}

#gridstack-wrapper{
	position:relative;
	/* 주먹구구식 변경 */
	left:14%;
	top:2%;
}

#pleaseLogin, #pleaseConfigure {
	height:100%;
}

iframe{
	width:95%;
	height:95%;
	box-shadow: 2px 2px 15px #b1c1ce;
	border-radius:4px;
	background-color:white;
	margin-top:3px;
	border:none;
}

#icons{
	margin-top:3px;
	padding-right:10px;
}

.smallIcon{
	width:40px;
	height:40px;
}

.bigIcon{
	width:120px;
	margin-bottom:140px;
}

.memberInput{
	padding:3px 5px 3px 5px;
	width:80;
	text-align:left;
	vertical-align:middle;
	border: 1px solid #d2d8d8;
	border-radius: 3px;
}

#passwordInput{
	margin-bottom:-15px;
}

.memberIcon{
	margin-bottom:-5px;
	margin-left:10px;
	margin-right:5px;
}
.box1:-webkit-autofill{
	-webkit-box-shadow:0 0 0px 1000px white inset;
}

/* 로그아웃 밑줄 사라지게 */
:link, :visited{
	text-decoration:none;
	color:#454545;
}